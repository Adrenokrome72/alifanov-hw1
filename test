CREATE TABLE Genre(
    GenreID INT PRIMARY KEY,
    TrackID INT FOREIGN KEY,
  	Genre_name VARCHAR(255)
)

CREATE TABLE Artist(
    ArtistID INT PRIMARY KEY,
    GenreID INT FOREIGN KEY,
  	Artist_name VARCHAR(255),
  	Artist_genre VARCHAR(255)
)

CREATE TABLE Album(
    AlbumID INT PRIMARY KEY,
    ArtistID INT FOREIGN KEY,
  	Album_name VARCHAR(255),
  	Release_year VARCHAR(255)
)

CREATE TABLE Track(
    TrackID INT PRIMARY KEY,
    AlbumID INT FOREIGN KEY,
  	Track_name VARCHAR(255),
  	Track_time VARCHAR(255),
  	Track_album VARCHAR(255)
)

CREATE TABLE Mixtape(
    MixtapeID INT PRIMARY KEY,
    TrackID INT FOREIGN KEY,
  	Mixtape_name VARCHAR(255),
  	Release_year VARCHAR(255),
  	Track_name VARCHAR(255)
)
